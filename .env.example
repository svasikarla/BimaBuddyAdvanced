# BimaBuddy Advanced - Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI Configuration (for chatbot)
OPENAI_API_KEY=your_openai_api_key

# ElevenLabs Configuration (for voice agent)
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Terra API Configuration (for wellness integration)
# Sign up at https://tryterra.co to get your credentials
NEXT_PUBLIC_TERRA_DEV_ID=your_terra_dev_id
TERRA_API_KEY=your_terra_api_key

# Fitbit API Configuration (optional - for direct integration)
# Register at https://dev.fitbit.com
FITBIT_CLIENT_ID=your_fitbit_client_id
FITBIT_CLIENT_SECRET=your_fitbit_client_secret

# Analytics & Monitoring (optional)
# NEXT_PUBLIC_GA_MEASUREMENT_ID=your_google_analytics_id
# SENTRY_DSN=your_sentry_dsn
